<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/add_nav_controller"
    app:startDestination="@id/categoryAddFragment">

    <fragment
        android:id="@+id/categoryAddFragment"
        android:name="com.bshapp.fragments.CategoryAddFragment"
        android:label="fragment_category_add"
        tools:layout="@layout/fragment_category_add" >
        <action
            android:id="@+id/action_categoryAddFragment_to_addNewsFragment"
            app:destination="@id/addNewsFragment" />
        <action
            android:id="@+id/action_categoryAddFragment_to_addRealStateFragment"
            app:destination="@id/addRealStateFragment" />
        <action
            android:id="@+id/action_categoryAddFragment_to_addAdsFragment"
            app:destination="@id/addAdsFragment" />
        <action
            android:id="@+id/action_categoryAddFragment_to_addJobsFragment2"
            app:destination="@id/addJobsFragment" />
    </fragment>
    <fragment
        android:id="@+id/addNewsFragment"
        android:name="com.bshapp.fragments.addNewsFragment"
        android:label="fragment_add_news"
        tools:layout="@layout/fragment_add_news" >
        <action
            android:id="@+id/action_addNewsFragment_to_sentSuccessfullyFragment"
            app:destination="@id/sentSuccessfullyFragment" />
    </fragment>
    <fragment
        android:id="@+id/sentSuccessfullyFragment"
        android:name="com.bshapp.fragments.SentSuccessfullyFragment"
        android:label="fragment_sent_successfully"
        tools:layout="@layout/fragment_sent_successfully" />
    <fragment
        android:id="@+id/addRealStateFragment"
        android:name="com.bshapp.fragments.AddRealStateFragment"
        tools:layout="@layout/fragment_add_realestate"
        android:label="AddRealStateFragment" />
    <fragment
        android:id="@+id/addAdsFragment"
        android:name="com.bshapp.fragments.AddAdsFragment"
        android:label="fragment_add_ads"
        tools:layout="@layout/fragment_add_ads" />
    <fragment
        android:id="@+id/addJobsFragment"
        android:name="com.bshapp.fragments.addJobsFragment"
        android:label="fragment_add_jobs"
        tools:layout="@layout/fragment_add_jobs" />
</navigation>